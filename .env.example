# Cloudflare Configuration
# Copy this file to .env for local development
# For production, use: npx wrangler secret put <KEY_NAME>

# Required: Cloudflare API Token with Gateway:Read permission
CF_API_TOKEN=your_cloudflare_api_token_here

# Required: Your Cloudflare Account ID
CF_ACCOUNT_ID=your_account_id_here

# Optional: Use Log Explorer (default: true)
USE_LOG_EXPLORER=true

# Optional: Environment name
ENVIRONMENT=development
